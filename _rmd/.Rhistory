ggplot(data=df, aes(x=AdjO, y=AdjD, color = Region)) + geom_point(size=2, alpha=0.7) +  scale_y_reverse() + xlab("Points scored per 100 possessions") + ylab("Points allowed per 100 possessions") + theme_bw() + geom_text_repel(aes(label=ifelse(AdjD<90 | AdjO > 122 | AdjD > 110 ,as.character(Team),"")), show.legend = FALSE) + labs(title = "Adjusted Defense vs. Adjusted Offense", subtitle = "Adjusted for Strength of Schedule", caption = "Source: KenPom")
is_outlier = function(x) {
return(x < quantile(x, 0.25) - 1.5 * IQR(x) | x > quantile(x, 0.75) + 1.5 * IQR(x))
}
df2 = df  %>% group_by(Region) %>% mutate(is_outlier=ifelse(is_outlier(AdjT), AdjT, as.numeric(NA)))
df2$outlier[which(is.na(df2$is_outlier))] = as.numeric(NA)
ggplot(data=df, aes(x=Region,y=AdjT)) + geom_boxplot() + geom_text_repel(aes(label=outlier),na.rm=TRUE)
