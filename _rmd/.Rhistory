library(ggplot2)
library(RCurl)
library(ggrepel)
df = read.csv(text=getURL("https://raw.githubusercontent.com/jskaza/data/master/ncaa2018/seeds2018.csv"))
ggplot(data=df, aes(x=AdjO, y=AdjD, color = Region)) + geom_point(size=2, alpha=0.7) +  scale_y_reverse() + xlab("Points scored per 100 possessions") + ylab("Points allowed per 100 possessions") + theme_bw() + geom_text_repel(aes(label=ifelse(AdjD<90 | AdjO > 122 | AdjD > 110 ,as.character(Team),"")), show.legend = FALSE) + labs(title = "Adjusted Defense vs. Adjusted Offense", subtitle = "Adjusted for Strength of Schedule", footnote = "Source: KenPom")
